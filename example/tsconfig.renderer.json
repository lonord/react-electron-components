{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "./lib/renderer",
    "rootDir": "../",
    "removeComments": true,
    "inlineSourceMap": true,
    "plugins": [{
      "name": "typescript-styled-plugin"
    }]
  },
  "include": [
    "../**/*.ts",
    "../**/*.tsx"
  ],
  "exclude": [
    "./src/main"
  ]
}